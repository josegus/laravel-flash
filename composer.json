{
    "name": "josegus/laravel-flash",
    "require": {
        "php": ">=7.0",
        "illuminate/session": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gustavo",
            "email": "jgvv15@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "JoseGus\\LaravelFlash\\": "src/"
        },
        "files": [
            "helpers/flash.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "JoseGus\\LaravelFlash\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JoseGus\\LaravelFlash\\LaravelFlashServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
